    "chai": "^4.1.2",
    "css-loader": "~0.21.0",
    "eslint": "1.7.3",
    "eslint-config-angular": "0.4.0",
    "eslint-plugin-angular": "0.12.0",
    "file-loader": "~0.8.4",
