{
  "name": "webpack-starter-angular",
  "version": "1.0.0",
  "author": "Timo Stollenwerk",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-sanitize": "1.5.8",
    "angular-schema-form": "0.8.13",
    "angular-schema-form-bootstrap": "0.2.0",
    "angular-ui-bootstrap": "1.1.2",
    "angular-ui-router": "0.3.1",
    "bootstrap": "3.3.7",
    "jquery": "3.1.0",
    "lodash": "4.14.1",
    "objectpath": "1.2.1",
    "tv4": "1.2.7"
  },
  "devDependencies": {
    "angular-mocks": "~1.5.0",
    "babel-core": "^6.3.26",
    "babel-loader": "^6.1.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^0.12.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^1.7.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.5",
    "karma-sourcemap-loader": "^0.3.4",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.3.0",
    "node-sass": "^3.3.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "~1.13.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^0.14.0"
  },
  "keywords": [
    "angular",
    "bundle",
    "bundling",
    "npm",
    "webpack"
  ],
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "analyze": "webpack --json | webpack-bundle-size-analyzer",
    "build": "export NODE_ENV=dev && webpack",
    "build:production": "export NODE_ENV=prod && webpack -p",
    "clean": "npm cache clean && rimraf -- node_modules doc typings coverage dist",
    "lint": "eslint src/ || true",
    "lint:travis": "eslint src/",
    "start": "webpack-dev-server --inline --port 3000 --content-base dist/",
    "test": "karma start",
    "test:debug": "karma start karma.conf.js --log-level=debug --single-run=False",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=False"
  }
}
